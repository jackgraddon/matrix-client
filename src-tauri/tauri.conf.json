{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "matrix-client",
  "version": "0.1.0",
  "identifier": "cc.jackg.matrixclient",
  "build": {
    "frontendDist": "../.output/public",
    "devUrl": "https://localho.st:3000",
    "beforeDevCommand": "corepack yarn dev",
    "beforeBuildCommand": "corepack yarn generate"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Matrix Client",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}